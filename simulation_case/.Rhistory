plot_voi <- function(df){
gg <- ggplot(df) + aes(x = time, y = voi, color = type) + geom_line() + geom_point() +
theme_bw() + labs(x = "Time", y = "VOI") +
coord_cartesian(ylim = c(0, 2.5)) +
scale_x_continuous(breaks = df$time) + labs(color = NULL) +
scale_color_discrete(labels = c('Lower seismic', 'Higher seismic', 'Lower csem', 'Higher csem')) +
geom_hline(yintercept = 2.1,  color = '#999999', linetype = 'longdash') + geom_hline(yintercept = 2.0,  color = '#666666', linetype = 'dotted') +
geom_hline(yintercept = 0.5,  color = '#999999', linetype = 'longdash') + geom_hline(yintercept = 0.4,  color = '#666666', linetype = 'dotted') +
annotate("text", x = 2024, y = 2.05, label = "Prices seismic", color = '#999999', size = 4) +
annotate("text", x = 2024, y = 0.45, label = "Prices csem", color = '#999999', size = 4) +
theme(legend.title = element_text(size = 14), legend.text = element_text(size = 12), legend.position = 'bottom',
axis.title = element_text(size = 14), axis.text = element_text(size = 12),
panel.grid.minor.y = element_blank(), panel.grid.minor.x = element_blank())
return(gg)
}
